{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./@types"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "module": "es2022"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src", 
    "@types"
  ]
}
